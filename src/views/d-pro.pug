        // SEARCH
        .d-search
          .container
            .d-title Find jobs
            .d-search__box.d-box
              .d-search__top
                .d-search__main
                  input(type='text' placeholder='i.e. MD, California, Internal Medicine ')
                  button(type='submit')
                    i.icon.icon-search
                a(href='#').btn.btn--ghost.btn--150.js-toggleSearchFilters Advanced search
              .d-search__collapsable
                .d-search__collapsable__wrapper
                  .d-search__col
                    .ui-group.ui-filtrable
                      label Job Title
                      a(href='#' data-id='') Physicians
                      a(href='#' data-id='').active Advanced Practitioners
                      a(href='#' data-id='') Allied Health
                      a(href='#' data-id='') Nursing
                  .d-search__col
                    .ui-group
                      label Specialty
                      .ui-select
                        .ui-select__visible 
                          span Select Allied Health Specialty
                          i.icon.icon-select
                        .ui-select__dropdown
                          span.ui-select__dropdown__placeholder Select Allied Health Specialty
                          span(data-value='Specialty 1') Specialty 1
                          span(data-value='Specialty 2') Specialty 2
                          span(data-value='Specialty 3') Specialty 3
                          span(data-value='Specialty 4') Specialty 4
                          span(data-value='Specialty 5') Specialty 5
                    .ui-group.js-showMoreList
                      label Other
                      .ui-radio(data-visible='1')
                        input(type='checkbox' name='checkbox_1' id='checkbox_1')
                        label(for='checkbox_1') Board Certified
                      .ui-radio(data-visible='1')
                        input(type='checkbox' name='checkbox_2' id='checkbox_2')
                        label(for='checkbox_2') Board Eligible
                      .ui-radio(data-visible='0')
                        input(type='checkbox' name='checkbox_3' id='checkbox_3')
                        label(for='checkbox_3') Board Certified
                      .ui-radio(data-visible='0')
                        input(type='checkbox' name='checkbox_4' id='checkbox_4')
                        label(for='checkbox_4') Board Eligible
                      .show-more(data-alttext='Show less' data-hidden='1')
                        a(href='#') See more
