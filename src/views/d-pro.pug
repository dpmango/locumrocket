    .page.page--bg.page--pad.static-header
      .page__content
        // DASHBOARD ALERTS
        .alerts
          .alert.alert--green.visible
            i.icon.icon-alert
            .alert__text A candidate has accepted your job. Go to 
              a(href='#') My Jobs/Filled
            i.icon.icon-close
          .alert.alert--red.js-toggableAlert
            i.icon.icon-alert
            .alert__text You have unapproved timesheet(s). Go to
              a(href='#') BILLING
            i.icon.icon-close
          .alert.alert--red.js-toggableAlert
            i.icon.icon-alert
            .alert__text You have unpaid job(s). Go to
              a(href='#') BILLING
            i.icon.icon-close
          .alerts__toggler(data-hidden='1')
            a(href='#') show 2 more
        
        // DASHBOARD
        .d-welcome
          .container
            .d-title My Dashboard
            .d-welcome__wrapper
              // facility image and maps
              .d-welcome__left
                .d-welcome__facility-image.d-box
                  img(src='images/tmp/dashHospital.jpg' srcset='images/tmp/dashHospital@2x.jpg 2x')
                .d-welcome__facility-map.d-box
                  // GOOGLE API
                  script(type='text/javascript', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAdDq67GS9ongMkjzokPsC5fTxdvEKidfI')
                  script(type='text/javascript', src='js/maps.js')
                  .js-facilityMap
              // facility description
              .d-welcome__right
                .d-welcome__about.d-box
                  .d-welcome__about__head
                    .d-welcome__about__head__name Facility details
                    a(href='#').btn.btn--red.btn--150.btn--head Edit
                  // row
                  .d-welcome__about__row.d-welcome__about__row--2col
                    .d-welcome__about__item
                      .d-welcome__about__label Facility
                      .d-welcome__about__value BELMONT ALBANY VILLAGE
                    .d-welcome__about__item
                      .d-welcome__about__label Type
                      .d-welcome__about__value Community Hospital
                  // row
                  .d-welcome__about__row.d-welcome__about__row--3col
                    .d-welcome__about__item
                      .d-welcome__about__label Doctors
                      .d-welcome__about__value 15
                    .d-welcome__about__item
                      .d-welcome__about__label Clinical staff
                      .d-welcome__about__value 200
                    .d-welcome__about__item
                      .d-welcome__about__label Rooms
                      .d-welcome__about__value 500
                  // row
                  .d-welcome__about__row
                    .d-welcome__about__item
                      .d-welcome__about__label Adress
                      .d-welcome__about__value 23, University Avenue, Berkeley, 94608
                  // row
                  .d-welcome__about__row
                    .d-welcome__about__item
                      .d-welcome__about__label Description
                      .d-welcome__about__value 
                        p BELMONT ALBANY VILLAGE is the only Level One trauma center in Berkeley and Northern San Mateo County. We provide life-saving medical care to the 1.5 million adults and children of Berkeley, San Francisco and northern San Mateo County. We offer comprehensive tertiary services designed to meet the needs of diverse communities. Services include 24-hour emergency care, minimally invasive surgery, high risk obstetrics and Level III Newborn Intensive Care Nursery
                  // row
                  .d-welcome__about__row.d-welcome__about__row--contacts
                    .d-welcome__about__item
                      .d-welcome__about__label Contact
                      .d-welcome__about__value 
                        | Mary Tyller
                        p Sr. Vice President
                    .d-welcome__about__item
                      .d-welcome__about__value 
                        | (510) 345-564
                        p Phone
                    .d-welcome__about__item
                      .d-welcome__about__value 
                        a(href='mailto:mtyller@vill.com') mtyller@vill.com
                        p Email
                      
            
        // SEARCH
        .d-search
          .container
            .d-title Find a medical professional
            .d-search__box.d-box
              .d-search__top
                .d-search__main
                  input(type='text' placeholder='i.e. MD, California, Internal Medicine ')
                  button(type='submit')
                    i.icon.icon-search
                a(href='#').btn.btn--ghost.btn--150.js-toggleSearchFilters Advanced search
              .d-search__collapsable
                .d-search__collapsable__wrapper
                  .d-search__col
                    .ui-group.ui-filtrable
                      label Job Title
                      a(href='#' data-id='') Physicians
                      a(href='#' data-id='').active Advanced Practitioners
                      a(href='#' data-id='') Allied Health
                      a(href='#' data-id='') Nursing
                  .d-search__col
                    .ui-group
                      label Specialty
                      .ui-select
                        .ui-select__visible 
                          span Select Allied Health Specialty
                          i.icon.icon-select
                        .ui-select__dropdown
                          span.ui-select__dropdown__placeholder Select Allied Health Specialty
                          span(data-value='Specialty 1') Specialty 1
                          span(data-value='Specialty 2') Specialty 2
                          span(data-value='Specialty 3') Specialty 3
                          span(data-value='Specialty 4') Specialty 4
                          span(data-value='Specialty 5') Specialty 5
                    .ui-group.js-showMoreList
                      label Other
                      .ui-radio(data-visible='1')
                        input(type='checkbox' name='checkbox_1' id='checkbox_1')
                        label(for='checkbox_1') Board Certified
                      .ui-radio(data-visible='1')
                        input(type='checkbox' name='checkbox_2' id='checkbox_2')
                        label(for='checkbox_2') Board Eligible
                      .ui-radio(data-visible='0')
                        input(type='checkbox' name='checkbox_3' id='checkbox_3')
                        label(for='checkbox_3') Board Certified
                      .ui-radio(data-visible='0')
                        input(type='checkbox' name='checkbox_4' id='checkbox_4')
                        label(for='checkbox_4') Board Eligible
                      .show-more(data-alttext='Show less' data-hidden='1')
                        a(href='#') See more
                  .d-search__col
                    .ui-group
                      label Town
                      input(type='text' placeholder='Berkeley').ui-input
                    .ui-group
                      label State
                      input(type='text' placeholder='CA').ui-input  
                  .d-search__col
                    .ui-group
                      label Weekly Time commitment
                      .ui-radio
                        input(type='checkbox' name='radio' id='radio_1')
                        label(for='radio_1') Less than 5 hours
                      .ui-radio
                        input(type='checkbox' name='radio' id='radio_2')
                        label(for='radio_2') 5 to 10 hours
                      .ui-radio
                        input(type='checkbox' name='radio' id='radio_3')
                        label(for='radio_3') Over 10 hours
                      .ui-radio
                        input(type='checkbox' name='radio' id='radio_4')
                        label(for='radio_4') Flexible
                    
                .d-search__collapsable__cta
                  .d-search__collapsable__cta__applied
                    label Filters applied
                    .ui-tag
                      span 10+ h commitment
                      i.icon.icon-close
                  .d-search__collapsable__cta__btns
                    a(href='#').btn.btn--ghost.btn--150 RESET
                    a(href='#').btn.btn--ghost.btn--150 UPDATE
                    

